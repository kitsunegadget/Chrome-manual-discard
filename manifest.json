{
    "manifest_version": 2,
    "name": "Chrome manual discard",
    "version": "1.2",
    "description": 
        "表示していないタブのメモリを手動で開放します.",
    "icons": {
        "16": "icon128.png",
        "48": "icon128.png",
        "128": "icon128.png"
    },
    "author": "Kitsune Gadget",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_icon" : {

        },
        "default_title": "非表示タブのメモリを開放します（作業ページがある場合は先に表示しておいてください）"
    },
    "permissions": ["activeTab", "notifications"]
}
